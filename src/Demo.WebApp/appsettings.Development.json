{
  "Logging": {
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
          "IncludeScopes": true,
          "TimestampFormat": "yyyy-MM-dd HH:mm:sszzz ",
          "UseUtcTimestamp": true
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
